#include <stdio.h>
#include "holberton.h"

int set_bit(unsigned long int *n, unsigned int index)
{
	unsigned int contabit = 0;

	if (index < 64)
	{
		while (n[contabit] != '\0')
		{
			if (contabit == index)
			{
				n[contabit] = n[contabit] | 1;
			}
			contabit++;
		}
		return (1);
	}
	return (-1);
}
